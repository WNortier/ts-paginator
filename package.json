{
  "name": "ts-paginator",
  "version": "1.5.1",
  "description": "TypeScript pagination module with helper functions intended for frontend development",
  "main": "lib/ts-paginator.js",
  "types": "lib/ts-paginator.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "build": "rm -rf lib && tsc",
    "format": "prettier --write \"**/*.ts\" \"**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WNortier/ts-paginator.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "pagination",
    "frontend",
    "react",
    "nextjs"
  ],
  "author": "warwick nortier",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WNortier/ts-paginator/issues"
  },
  "homepage": "https://github.com/WNortier/ts-paginator#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "ts-paginator": "^1.1.10"
  }
}
